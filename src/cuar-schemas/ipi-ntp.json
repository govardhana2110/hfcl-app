                              "properties": {
                                "client-ip-address": {
                                  "description": "use this attribute to specify the allowed NTP client IP address",
                                  "readOnly": false,
                                  "oneOf": [
                                    {
                                      "pattern": "(([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(%[\\p{N}\\p{L}]+)?",
