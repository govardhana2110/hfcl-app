                                    },
                                    {
                                      "pattern": "(((:|[0-9a-fA-F]{0,4}):)([0-9a-fA-F]{0,4}:){0,5}((([0-9a-fA-F]{0,4}:)?(:|[0-9a-fA-F]{0,4}))|(((25[0-5]|2[0-4][0-9]|[01]?[0-9]?[0-9])\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9]?[0-9])))(%[\\p{N}\\p{L}]+)?)|((([^:]+:){6}(([^:]+:[^:]+)|(.*\\..*)))|((([^:]+:)*[^:]+)?::(([^:]+:)*[^:]+)?)(%.+)?)",
                                      "type": "string"
                                    }
                                  ],
                                  "mandatory": false
                                },
                                "netmask": {
                                  "description": "use this attribute to specify the allowed NTP client IP network mask",
                                  "readOnly": false,
                                  "oneOf": [
                                    {
                                      "pattern": "(([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(%[\\p{N}\\p{L}]+)?",
                                      "type": "string"
                                    },
                                    {
                                      "minimum": 1,
                                      "maximum": 128,
                                      "type": "number"
                                    }
                                  ],
                                  "mandatory": false
                                },
                                "access-options": {
                                  "description": "use this attribute to specify the allowed access options for NTP client",
                                  "readOnly": false,
                                  "type": "string",
                                  "bits": {
                                    "kod": "0",
                                    "nopeer": "1",
                                    "noserve": "2",
                                    "noquery": "3",
                                    "nomodify": "4",
                                    "notrap": "5",
                                    "limited": "6"
                                  },
                                  "mandatory": false
                                }
                              }
                            },
